{
  "mcpServers": {
    "Context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "10000"
      },
      "alwaysAllow": [
        "resolve-library-id",
        "get-library-docs"
      ]
    },
    "Toolbox": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery/toolbox",
        "--key",
        "af932539-c31a-4cf3-9019-b0bb0530d255",
        "--profile",
        "solid-limpet-6kfyJ3"
      ]
    },
    "Github": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery-ai/github",
        "--key",
        "af932539-c31a-4cf3-9019-b0bb0530d255",
        "--profile",
        "solid-limpet-6kfyJ3"
      ]
    },
    "Exa Search": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "exa",
        "--key",
        "af932539-c31a-4cf3-9019-b0bb0530d255"
      ]
    }
  }
}